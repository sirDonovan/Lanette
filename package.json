{
  "name": "lanette",
  "version": "0.0.1",
  "description": "A bot for Pokemon Showdown",
  "main": "build/app.js",
  "scripts": {
    "build": "node build.js",
    "gameserver": "npm run build && node --enable-source-maps build/src/game-server/server.js",
    "lint": "eslint \"**/*.ts\" --cache",
    "localboot": "npm run build && node --enable-source-maps --report-uncaught-exception --report-on-fatalerror build/temp-config.js -- --offline",
    "localtest": "npm run lint && npm run tsc && npm run build && npm run mocha -- --offline",
    "mocha": "node --enable-source-maps build/mocha.js -- --ci",
    "start": "npm run build && node --enable-source-maps --report-uncaught-exception --report-on-fatalerror build/app.js",
    "test": "npm run lint && npm run tsc && npm run build && npm run mocha",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sirDonovan/Lanette.git"
  },
  "author": "Quinton Lee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sirDonovan/Lanette/issues"
  },
  "homepage": "https://github.com/sirDonovan/Lanette#readme",
  "dependencies": {
    "@types/eslint": "8.4.10",
    "@types/mocha": "10.0.1",
    "@types/node": "18.11.17",
    "@types/ws": "8.5.3",
    "bufferutil": "4.0.7",
    "esbuild": "0.16.10",
    "typescript": "4.9.4",
    "utf-8-validate": "5.0.10",
    "ws": "8.11.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.47.0",
    "@typescript-eslint/parser": "5.47.0",
    "eslint": "8.30.0",
    "mocha": "10.2.0"
  }
}
