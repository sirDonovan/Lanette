{
  "name": "lanette",
  "version": "0.0.1",
  "description": "A bot for Pokemon Showdown",
  "main": "app.js",
  "scripts": {
    "citest": "node --enable-source-maps mocha.js -- --noBuild",
    "lint": "eslint \"src/**/*.ts\" --cache",
    "localboot": "node --enable-source-maps tempConfig.js -- --offline --incrementalBuild",
    "localtest": "npm run lint && node --enable-source-maps mocha.js -- --offline --incrementalBuild",
    "start": "node app.js",
    "test": "npm run lint && node --enable-source-maps mocha.js",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sirDonovan/Lanette.git"
  },
  "author": "Quinton Lee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sirDonovan/Lanette/issues"
  },
  "homepage": "https://github.com/sirDonovan/Lanette#readme",
  "dependencies": {
    "@types/mocha": "9.1.0",
    "@types/node": "16.11.26",
    "@types/ws": "8.5.2",
    "bufferutil": "4.0.6",
    "typescript": "4.6.2",
    "utf-8-validate": "5.0.8",
    "ws": "8.5.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.13.0",
    "@typescript-eslint/parser": "5.13.0",
    "eslint": "8.10.0",
    "heapdump": "0.3.15",
    "mocha": "9.2.1"
  }
}
